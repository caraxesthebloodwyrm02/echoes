{
  "$schema": null,
  "min_maintainability": 30.0,
  "max_complexity": 20.0,
  "min_pylint_score": 4.0,
  "max_duplicate_percentage": 25.0,
  "remove_dead_code": true,
  "create_backups": true,
  "report_format": "json",
  "excluded_dirs": [
    "venv",
    "__pycache__",
    ".git",
    "node_modules",
    ".vscode",
    "dist",
    "build",
    ".code_quality_backup",
    "quality_reports"
  ],
  "excluded_files": [
    "test_",
    "__init__.py",
    "conftest.py"
  ],
  "quality_weights": {
    "maintainability": 0.3,
    "complexity": 0.25,
    "pylint_score": 0.25,
    "duplicates": 0.1,
    "dead_code": 0.1
  },
  "auto_cleanup_rules": {
    "remove_if_maintainability_below": 15.0,
    "remove_if_pylint_below": 2.0,
    "remove_if_issues_count_above": 5,
    "refactor_if_duplicate_above": 30.0
  },
  "safe_mode": true,
  "preserve_important_files": [
    "ai_agent_custom.py",
    "research_optimizer.py",
    "duplicate_detector.py",
    "standard_checker.py",
    "loadtest/locustfile.py"
  ]
}
